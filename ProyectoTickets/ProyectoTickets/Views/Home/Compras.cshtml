@{
}

@model List<ProyectoTickets.Models.Usuario> 

<h1>Tablas almacenadas en Base de Datos</h1>

<table class="table table-striped">
    <thead>

    </thead>
    <tr>
        <th>Nombre</th>
        <th>Correo</th>
        <th>Contraseña</th>
        <th>NúmeroTeléfono</th>
    </tr>
    
    <tbody>
        @foreach(var item in Model)
        {
            <tr>
                <td>@item.Nombre</td>
                <td>@item.Correo</td>
                <td>@item.Contraseña</td>
                <td>@item.NúmeroTeléfono</td>

            </tr>

        }

    </tbody>

</table>
